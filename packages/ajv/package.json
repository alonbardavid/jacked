{
  "name": "@jacked/ajv",
  "version": "0.1.0",
  "description": "Jacked ajv validator",
  "main": "dist/cjs/index.js",
  "module": "dist/module/index.js",
  "typings": "dist/types/index.d.ts",
  "private":false,
  "scripts": {
    "build": "npm-run-all -p \"build:*\"",
    "build:es6": "tsc -p ./tsconfig.json",
    "build:cjs": "tsc -p ./tsconfig.cjs.json",
    "clean": "rimraf dist/*",
    "test": "jest"
  },
  "devDependencies": {
    "@types/jest": "^22.2.2",
    "babel-jest": "^22.4.3",
    "jest": "^22.4.3",
    "npm-run-all": "^4.1.2",
    "rimraf": "^2.6.2",
    "ts-jest": "^22.4.2",
    "tsc": "^1.20150623.0",
    "typescript": "^2.8.1"
  },
  "dependencies": {
    "@jacked/core": "^0.1.0",
    "ajv": "^6.4.0",
    "lodash.mapvalues": "^4.6.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ],
    "transform": {
      "^.+\\.jsx?$": "<rootDir>/../../node_modules/babel-jest",
      "^.+\\.tsx?$": "<rootDir>/../../node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": ".*\\.spec\\.(js|ts|tsx)$",
    "moduleDirectories": [
      "node_modules",
      "."
    ]
  },
  "license": "MIT"
}
